{
  "name": "ngx-speculoos-project",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "build": "ng build --project=ngx-speculoos",
    "test": "ng test --no-progress --no-watch --code-coverage --project=ngx-speculoos",
    "watch": "ng build --project=ngx-speculoos --watch --configuration development",
    "demobuild": "ng build --project=demo",
    "demotest": "ng test --no-progress --no-watch --project=demo",
    "lint": "ng lint",
    "release": "cd projects/ngx-speculoos && standard-version --infile ../../CHANGELOG.md",
    "doc": "node scripts/prepare-doc.js && cd projects/ngx-speculoos && compodoc",
    "codecov": "codecov",
    "tsd": "tsc -p projects/tsd-test/tsconfig.tsd.json",
    "format": "prettier --write ./"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "14.0.0",
    "@angular/compiler": "14.0.0",
    "@angular/core": "14.0.0",
    "@angular/forms": "14.0.0",
    "@angular/platform-browser": "14.0.0",
    "@angular/platform-browser-dynamic": "14.0.0",
    "@angular/router": "14.0.0",
    "rxjs": "7.5.5",
    "tslib": "2.4.0",
    "zone.js": "0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "14.0.0",
    "@angular-eslint/builder": "13.2.1",
    "@angular-eslint/eslint-plugin": "13.2.1",
    "@angular-eslint/eslint-plugin-template": "13.2.1",
    "@angular-eslint/schematics": "13.2.1",
    "@angular-eslint/template-parser": "13.2.1",
    "@angular/cli": "14.0.0",
    "@angular/compiler-cli": "14.0.0",
    "@compodoc/compodoc": "1.1.19",
    "@types/jasmine": "4.0.3",
    "@types/node": "16.11.38",
    "@typescript-eslint/eslint-plugin": "5.27.0",
    "@typescript-eslint/parser": "5.27.0",
    "codecov": "3.8.3",
    "eslint": "8.16.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-ban": "1.6.0",
    "eslint-plugin-deprecation": "1.3.2",
    "eslint-plugin-prettier": "4.0.0",
    "jasmine-core": "4.1.1",
    "karma": "6.3.20",
    "karma-chrome-launcher": "3.1.1",
    "karma-coverage": "2.2.0",
    "karma-jasmine": "5.0.1",
    "karma-jasmine-html-reporter": "2.0.0",
    "ng-packagr": "14.0.0",
    "prettier": "2.6.2",
    "standard-version": "9.5.0",
    "typescript": "4.6.4"
  }
